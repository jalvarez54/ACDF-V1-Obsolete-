@model IkoulaACDF.Models.HomePageViewModel

@foreach (var item in Model.AspNetUsers)
{

    <div class="panel panel-default">
        <div class="panel-heading">
            <img src="@item.PhotoUrl" title="@item.FirstName @item.LastName" alt="@item.UserName" width="25" height="25" />
        </div>
        <div class="panel-body">
            <b>@Html.DisplayFor(modelItem => item.RegistrationDate)</b><br />
        </div>
        <div class="panel-footer">
            <b>@Html.DisplayFor(modelItem => item.UserName)</b><br />
        </div>
    </div>
}
<hr />