@model IkoulaACDF.Models.HomePageViewModel

@foreach (var item in @Model.Photos)
{
    <div class="thumbnail">
        @{string myTitle = item.Description + " : " + item.Place;}
        <a class="fancybox" title="@myTitle" href="@item.Path">
            <img src="@item.ThumbPath" alt="" />
        </a>
        <div class="caption">
            <h4>@item.UserName</h4>
            <p>@item.Date</p>
        </div>
    </div>
}
<hr />
